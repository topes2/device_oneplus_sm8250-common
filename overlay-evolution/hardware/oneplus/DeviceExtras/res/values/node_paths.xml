<!-- These resources are around just to allow their values to be customized
     for different OnePlus serires/devices.  Do not translate. -->
<resources>

    <!-- Kcal RED sysfs node path -->
    <string name="node_red_preference">/sys/module/msm_drm/parameters/kcal_red</string>

    <!-- Kcal GREEN sysfs node path -->
    <string name="node_green_preference">/sys/module/msm_drm/parameters/kcal_green</string>

    <!-- Kcal BLUE sysfs node path -->
    <string name="node_blue_preference">/sys/module/msm_drm/parameters/kcal_blue</string>

    <!-- Kcal SATURATION sysfs node path -->
    <string name="node_saturation_preference">/sys/module/msm_drm/parameters/kcal_sat</string>

    <!-- Kcal CONTRAST sysfs node path -->
    <string name="node_contrast_preference">/sys/module/msm_drm/parameters/kcal_cont</string>

    <!-- Kcal HUE sysfs node path -->
    <string name="node_hue_preference">/sys/module/msm_drm/parameters/kcal_hue</string>

    <!-- Kcal VALUE sysfs node path -->
    <string name="node_value_preference">/sys/module/msm_drm/parameters/kcal_val</string>

    <!-- DC-dimming sysfs node path -->
    <string name="node_dc_mode_switch">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/dimlayer_bl_en</string>

    <!-- High Brightnes sysfs node path -->
    <string name="node_hbm_mode_switch">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/hbm</string>

    <!-- Auto High Brightness sysfs node path -->
    <string name="node_auto_hbm_service">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/hbm</string>

    <!-- Measuered FPS sysfs node path -->
    <string name="node_fps_info_service">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/sde-crtc-0/measured_fps</string>

    <!-- TP Edge Limit procfs node path -->
    <string name="node_tp_edge_limit_mode_switch">/proc/touchpanel/tpedge_limit_enable</string>

    <!-- USB 2.0 Force fastcharge sysfs node path -->
    <string name="node_usb2_fast_charge_mode_switch">/sys/kernel/fast_charge/force_fast_charge</string>

    <!-- OTG sysfs node path -->
    <string name="otg_mode_switch">/sys/class/power_supply/usb/otg_switch</string>

    <!-- System vibration sysfs node path -->
    <string name="node_vibrator_strength_preference">/sys/devices/platform/soc/a8c000.i2c/i2c-3/3-005a/leds/vibrator/level</string>
</resources>
